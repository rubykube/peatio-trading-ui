<tr data-order="<%= index %>" data-price="<%= price %>" data-volume="<%= volume %>" class="<%= classes %>">
  <td class="amount col-xs-10"><div title="<%= formatter.unmask_amount(volume, price) %>">
    <%= formatter.amount(volume, price) %>
  </div></td>
  <td class="volume col-xs-7"><div title="<%= formatter.unmask_fixed_volume(volume) %>">
    <%= formatter.mask_fixed_volume(volume) %>
  </div></td>
  <td class="price col-xs-7 text-up"><div title="<%= formatter.unmask_fixed_price(price) %>">
    <%= formatter.mask_fixed_price(price) %>
  </div></td>
</tr>
